			</basic>
			<basic name="tipId">
				<column name="Tip_Id" />
			</basic>	
			 <one-to-many name="patternId" mapped-by="patternId"> </one-to-many>	
			 <transient name="ord"/>		
		</attributes>
	</entity>
	<entity class="org.iringtools.mapping.Ordinal">
